<!DOCTYPE html>
<html lang="zh-cn">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Test</title>
  </head>
  <body>
    <h2>Users</h2>
    {{each users}}
    <ul>
      <li>{{ $value["name"] }}</li>
      <li>{{ $value["age"] }}</li>
      <li>
        {{each $value['hobbies']}}
        <span> {{ $value }}</span>
        {{/each}}
      </li>
    </ul>
    {{/each}}
  </body>
</html>
